package Six;

public class TossSender extends ContentSender {
	String content;
	
	public TossSender() {
		
	}
	
	public TossSender(String bank, String name, String content) {
		super(bank, name);
		this.content = content;
	}

	@Override
	public void sendMsg(String recipient) {
		System.out.println("은행=" + this.bank);
		System.out.println("이름=" + this.name);
		System.out.println("내용=" + this.content);
		System.out.println("받는 사람=" + recipient);

	}

}
